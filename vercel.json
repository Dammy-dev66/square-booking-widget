{
  "version": 2,
  "builds": [
    { "src": "public/**/*", "use": "@vercel/static" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/services", "dest": "/api/services.js" },
    { "src": "/team-members", "dest": "/api/team-members.js" },
    { "src": "/checkout", "dest": "/api/checkout.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
